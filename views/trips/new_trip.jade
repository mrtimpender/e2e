extends ../layout
block content
	#main
		// START WRAPPER
		.wrapper
			include ../../public/includes/left_sidebar.jade
		section#content
			// start container
			.container
				.row
					.col.s12.m6.l6
						.card
							.card-move-up.teal.waves-effect.waves-block.waves-light
								.move-up
									h5.margin.white-text Start Location
									input#origin-input.controls(type='text', name='trip_name', placeholder='starting location for your trip')
					.col.s12.m6.l6
						.card
							.card-move-up.teal.waves-effect.waves-block.waves-light
								.move-up
									h5.margin.white-text Destination Location
									input#destination-input.controls(type='text', name='trip_name', placeholder='starting location for your trip')
									.input-field.col.s12
				.row
					.input-field.col.s5.m3.l4
						select#mode
							option(value='', disabled='', selected='') Select Your Mode
							option(value='TRANSIT', id='changemode-transit') Transit
							option(value='DRIVING', id='changemode-driving') Driving
							option(value='BICYCLING', id='changemode-bicycling') Bicycling
							option(value='WALKING', id='changemode-walking') Walking
						label 
					.col.s2.m2.offset-m1.l1.offset-l1
						a#getDirections.btn-floating.valign.btn-large.waves-effect.waves-light.cyan
							i.mdi-maps-directions
						//- button#showTraffic traffic
						//- button#showTransit transit
						//- button#showBike bike
					.col.s12.m6.l6
						#map(style="display:none")
						#jon_map.mobile-map
						#floating-panel
						.row
							button#displayDirections Get Directions
							#floating-panel
								strong Start:
								select#start
									option(value='chicago, il') Chicago
									option(value='st louis, mo') St Louis
									option(value='joplin, mo') Joplin, MO
									option(value='oklahoma city, ok') Oklahoma City
									option(value='amarillo, tx') Amarillo
									option(value='gallup, nm') Gallup, NM
									option(value='flagstaff, az') Flagstaff, AZ
									option(value='winona, az') Winona
									option(value='kingman, az') Kingman
									option(value='barstow, ca') Barstow
									option(value='san bernardino, ca') San Bernardino
									option(value='los angeles, ca') Los Angeles
								br
								strong End:
								select#end
									option(value='chicago, il') Chicago
									option(value='st louis, mo') St Louis
									option(value='joplin, mo') Joplin, MO
									option(value='oklahoma city, ok') Oklahoma City
									option(value='amarillo, tx') Amarillo
									option(value='gallup, nm') Gallup, NM
									option(value='flagstaff, az') Flagstaff, AZ
									option(value='winona, az') Winona
									option(value='kingman, az') Kingman
									option(value='barstow, ca') Barstow
									option(value='san bernardino, ca') San Bernardino
									option(value='los angeles, ca') Los Angeles
							.col.s12.m5
								#showDirections.card-panel.green.accent-4
									for in locations
									//- span.white-text(id='directionsDisplay')
							.col.s12.m6.l6
								#map(style="display:none")
								#jon_map.mobile-map
					
					
			// FOOTER
			include ../../public/includes/footer.jade
				//
					================================================
					Scripts
					================================================
				// jQuery Library
				script(type='text/javascript', src='/js/plugins/jquery-1.11.2.min.js')
				// materialize js
				script(type='text/javascript', src='/js/materialize.js')
				// prism
				script(type='text/javascript', src='/js/plugins/prism/prism.js')
				// scrollbar
				script(type='text/javascript', src='/js/plugins/perfect-scrollbar/perfect-scrollbar.min.js')
				// chartist
				script(type='text/javascript', src='/js/plugins/chartist-js/chartist.min.js')
				// chartjs
				script(type='text/javascript', src='/js/plugins/chartjs/chart.min.js')
				script(type='text/javascript', src='/js/plugins/chartjs/chart-script.js')
				// interactivity
				script(type='text/javascript', src='/js/interactivity.js')
				// sparkline
				script(type='text/javascript', src='/js/plugins/sparkline/jquery.sparkline.min.js')
				script(type='text/javascript', src='/js/plugins/sparkline/sparkline-script.js')
				// google map api - Believe its the templates
				//- script(type='text/javascript', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAAZnaZBXLqNBRXjd-82km_NO7GUItyKek')
				script(type='text/javascript', src='/js/mapsapi.js')
				//- script(type='text/javascript', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyB9xEAcDm6FwY9iDrdBRVsXhowU9FbDIao')
				// jvectormap
				script(type='text/javascript', src='/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js')
				script(type='text/javascript', src='/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js')
				script(type='text/javascript', src='/js/plugins/jvectormap/vectormap-script.js')
				// google map
				//- script(type='text/javascript', src='/js/plugins/google-map/google-map-script.js')
				// plugins.js - Some Specific JS codes for Plugin Settings
				script(type='text/javascript', src='/js/plugins.js')
				script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyB9xEAcDm6FwY9iDrdBRVsXhowU9FbDIao&libraries=places&callback=initMap', async='', defer='')
				//- script(src="/js/google_maps_api/auto_complete.js")
				script(src="/js/google_maps_api/new_trip_directions.js")
